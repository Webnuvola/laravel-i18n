{
    "name": "webnuvola/laravel-i18n",
    "description": "Laravel internationalization package",
    "keywords": [
        "laravel",
        "i18n",
        "php",
        "translation",
        "internationalization",
        "localization"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Fabio Cagliero",
            "email": "caglierofabio@gmail.com"
        }
    ],
    "require": {
        "illuminate/contracts": "5.5.*|5.6.*|5.7.*",
        "illuminate/routing": "5.5.*|5.6.*|5.7.*",
        "illuminate/support": "5.5.*|5.6.*|5.7.*",
        "illuminate/translation": "5.5.*|5.6.*|5.7.*",
        "illuminate/view": "5.5.*|5.6.*|5.7.*"
    },
    "autoload": {
        "psr-4": {
            "Webnuvola\\Laravel\\I18n\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webnuvola\\Laravel\\I18n\\I18nServiceProvider"
            ],
            "aliases": {
                "I18n": "Webnuvola\\Laravel\\I18n\\Facades\\I18n",
                "I18nRoute": "Webnuvola\\Laravel\\I18n\\Facades\\I18nRoute"
            }
        }
    }
}
